msgid_plural "{num} docs"
msgstr[0] "1 ஆவணம்"
msgstr[1] "{num} ஆவணங்கள்"

msgid "1 message"
msgid_plural "{num} messages"
msgstr[0] "1 சேதி"
msgstr[1] "{num} சேதிகள்"

